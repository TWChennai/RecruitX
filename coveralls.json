{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "def.+(.+\/\/.+).+do"
  ],

  "custom_stop_words": [
  ],

  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "minimum_coverage": 74.5
  },

  "skip_files": [
    "lib/recruitx_backend/endpoint.ex",
    "lib/swoosh_extensions/mailer.ex",
    "spec/*",
    "web/gettext.ex",
    "web/views/layout_view.ex",
    "web/web.ex",
  ]
}
